package ch02.ex03;

public class Solution {
	 public int solution(int num1, int num2) {
	        int answer = (num1/num2);
	        double answer2 =(num1%num2);
	        double answer3 = (int)answer + answer2;
	        double answer4 = answer3*1000;
	        		
	        	
	        return answer4;
	    }
}
