
public class ThreeD {

	public static void main(String[] args) {
		
		// royal 아파트는 4개층 4개호 방 3개
		String[][][] royal = new String[4][4][3];
		System.out.println(royal.length+" 층");
		System.out.println(royal[0].length+" 호");
		System.out.println(royal[0][0].length+" 개");

		// 2층 3호 2번방은 "철수의 방"
		royal[2][3][2]="철수의 방";
		// 0층 0호 0번방은 "영희의 방"
		royal[0][0][0]="영희의 방";
		
	}

}
