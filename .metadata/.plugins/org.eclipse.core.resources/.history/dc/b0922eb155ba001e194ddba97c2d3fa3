package ch02.ex03;

public class Main {

	public static void main(String[] args) {
		Solution sol = new Solution();
		int result = 0;
		sol.solution(3, 2);
		System.out.println("1500=="+result);
		sol.solution(7, 3);
		System.out.println("2333=="+result);
		sol.solution(10, 6);
		System.out.println("62=="+result);
	}

}
