
public class Assignment {

	public static void main(String[] args) {
	
		int result = 0; // 대입연산자
		
		// 복합대입 연산자
//		result = result + 10; // 10
		result += 10;
		System.out.println(result);
		
//		result = result - 5;
		result -= 5;
		System.out.println(result);
		
//		result = result * 2;
		result *= 2;
		System.out.println(result);
		
//		result = result / 2;
		result /= 2;
		System.out.println(result);
		
//		result = result%2;
		result %= 2;
		System.out.println(result);
		

	}

}
