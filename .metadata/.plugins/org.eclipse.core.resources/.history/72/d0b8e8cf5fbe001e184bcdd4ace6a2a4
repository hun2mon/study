package chap06.ex01;

public class Main {

	public static void main(String[] args) {
		
		// Mamal 의 기능을 사용해 보자.
		
		Mamal mamal = new Mamal();
		mamal.birth();
		mamal.eat();
		
		System.out.println();
		
		// Person
		
		Person per = new Person();
		per.useTool();
		per.social();
		per.talk();
		
		System.out.println();
		
		per.birth();
		per.eat();
		// 상속에서 내것처럼 쓴다 == 상속받은 자식(클래스)을 객체화 하면 부모것도 사용 할 수 있다.
		
		

	}

}
