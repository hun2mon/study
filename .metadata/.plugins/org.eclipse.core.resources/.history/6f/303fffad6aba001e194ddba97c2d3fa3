
public class StmtSwitch {

	public static void main(String[] args) {

		String item = "캔커피";
		
		
		// jdk 1.6 이하에서는 switch 문에 숫자, 문자만 사용 가능하다.
		switch (item) {
		case "콜라":
			System.out.println(item+ " 나왔습니다.");
			break;
		case "생수":
			System.out.println(item+ " 나왔습니다.");
			break;
		case "오렌지주스":
			System.out.println(item+ " 나왔습니다.");
			break;
		case "사이다":
			System.out.println(item+ " 나왔습니다.");
			break;
		case "에너지음료":
			System.out.println(item+ " 나왔습니다.");
			break;
		case "냉커피":
			System.out.println(item+ " 나왔습니다.");
			break;
		case "식혜":
			System.out.println(item+ " 나왔습니다.");
			break;
			
		default:
			System.out.println(item+" 보유하고 있지 않습니다.");
			break;
		}
		
		

	}

}
