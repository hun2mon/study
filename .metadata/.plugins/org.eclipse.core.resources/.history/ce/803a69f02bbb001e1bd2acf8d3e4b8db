package ch04.ex01;

public class Solution {
	public int solution(int[] dot) {
        int answer = 0;
        
        if (dot[0]>0 && dot[1]>0) {
			System.out.println(1);
		}else if (dot[0]<0 && dot[1]>0) {
			System.out.println(2);
		}else if ((dot[0]<0 && dot[1]<0) {
			System.out.println(3);
		}else {
			System.out.println(4);
		}
        
        return answer;
    }
}
