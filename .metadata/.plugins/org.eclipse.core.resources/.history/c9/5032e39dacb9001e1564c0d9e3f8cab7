
public class One {

	public static void main(String[] args) {

		int x = 10;
		int y = 10;
		
		x++; // 증가 나중에
		++y; // 증가 먼저
		System.out.println("x="+x);
		System.out.println("y="+y);
		
		x=1;
		y=1;
		int result1 = (++x)+10;
		int result2 = (y++)+10;
		
		System.out.println("result1 = "+result1);
		System.out.println("result2 = "+result2);
		System.out.println(x);
	}
	

}
