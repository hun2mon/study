package ch03.ex01;

public class Solution {
	public int solution(int angle) {
        int answer = 0;
        
        if (angle<90) {
			answer = 1;
        	System.out.println(answer);
		}else if (angle==90) {
			answer = 2;
		    System.out.println(answer);
		}else if (angle<180) {
			answer = 3;
		    System.out.println(answer);
		}else {
			answer = 4;
		    System.out.println(answer);
		}
        
        
        return answer;
    }
}
